                        </tr>
                    </table>
                    </div>

            </nav>

            <main role="main" class="col-md-8 ml-sm-auto col-lg-6 pt-3 px-4">
                <div class="chartjs-size-monitor" style="position: absolute; left: 0px; top: 0px; right: 0px; bottom: 0px; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -1;"><div class="chartjs-size-monitor-expand" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:1000000px;height:1000000px;left:0;top:0"></div></div><div class="chartjs-size-monitor-shrink" style="position:absolute;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1;"><div style="position:absolute;width:200%;height:200%;left:0; top:0"></div></div></div>
                @*<div id="container" style="height:400px"></div>*@
               
                <div>
                    <div class="card mb-4 box-shadow">
                        <div class="card-header">
                            <h4 class="my-0 font-weight-normal">ɃITCOIN PRICE <b>${{mainData.BctPrice}}</b></h4>
                        </div>
                        <div class="card-body">
                            <h1 class="card-title pricing-card-title">{{mainData.TotalSumBtc}} <small class="text-muted"> <b>Ƀ</b></small></h1>
                            <ul class="list-unstyled mt-3 mb-4">
                                <li>Total Bought VS Actual Value: <span style="background-color:#ffd800"><b>Ƀ</b> {{mainData.TotalBoughtOldValu}}</span> /<span ng-class="mainData.cssClass"> <b>Ƀ</b> {{mainData.TotalBoughtActualValue}}</span></li>
                                <li>Total Free: <b>Ƀ</b> {{mainData.TotalBtcFree}} / <b>$</b> {{mainData.TotalDollarFree}}</li>
                                <li>Total Account Dollar: <b>$</b> {{mainData.TotalSumDollar}}</li>
                                <li>Total Percentage: <span ng-class="mainData.cssClass"> {{mainData.TotalBoughtPerc}} <b>%</b></span></li>
                               
                            </ul>
                            @*<button type="button" class="btn btn-lg btn-block btn-outline-primary">Sign up for free</button>*@
                        </div>
                    </div>
                </div>
              
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
                    <button ng-show="selectedCoin!=undefined" ng-click="back();">Close</button>
                    <h1 class="h2"  ng-show="selectedCoin!=undefined"> <i ng-class="'cc '+selectedCoin.Logo" title="ADA"></i> {{selectedCoin.Symbol}}</h1> 
                     <div class="btn-toolbar mb-2 mb-md-0" ng-show="selectedCoin==undefined" >
                        <div class="btn-group mr-2">
                            <button class="btn btn-sm btn-outline-secondary">15m</button>
                            <button class="btn btn-sm btn-outline-secondary">30m</button>
                        </div>
                        <button class="btn btn-sm btn-outline-secondary dropdown-toggle">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-calendar"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"></rect><line x1="16" y1="2" x2="16" y2="6"></line><line x1="8" y1="2" x2="8" y2="6"></line><line x1="3" y1="10" x2="21" y2="10"></line></svg>
                            Day
                        </button>
                    </div>
                    <br />
                </div>
                <br />
                @*<canvas class="my-4 chartjs-render-monitor" id="myChart" width="2020" height="852" style="display: block; height: 568px; width: 1347px;"></canvas>*@


                <uib-tabset active="active">
                    <uib-tab index="0" heading="Charts" select="initChart()">
                        <div style="width:100%">
                            <h2>Investment Division</h2>
                            <div id="coinsDivision"></div>
                             <h2>Profit vs ActualPrice</h2>
                            <div id="coinsProfitContainer"></div>
                           
                        </div>

                    </uib-tab>
                    <uib-tab index="1" heading="Transactions">
                       
                         <h2>Target Transactions</h2>

                        <div class="table-responsive">
                            <table class="table table-striped table-sm">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Coin</th>
                                        <th>Amount</th>
                                        <th>Buy Value</th>
                                        <th>Sell Target</th>
                                        <th>Total Gain</th>
                                       <th>Distance %</th>
                                        <th>Target %</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="item in targetTransactions">
                                        <td>{{item.Date}}</td>
                                        <td>{{item.Symbol}}</td>
                                        <td>{{item.Amount}}</td>
                                        <td>{{item.BuyPrice}}</td>
                                        <td>{{item.SellPrice}}</td>
                                        <td>{{item.DiffTotal}}</td>
                                       <td ng-class="(item.ActualPrice - item.SellPrice)>0?'profit':'debit'">{{(item.ActualPrice - item.SellPrice)/item.SellPrice*100 | number:4}} %</td>
                                        <td ng-class="item.PercClass" style="background-color:#ffd800; color:#333333">{{item.Percentage}}</td>
                                    </tr>
                                  
                                </tbody>
                            </table>
                        </div>
                       
                        <h2>Sell History</h2>

                        <div class="table-responsive">
                            <table class="table table-striped table-sm">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Coin</th>
                                        <th>Amount</th>
                                        <th>Buy Value</th>
                                        <th>Sell Value</th>
                                        <th>Total Gain</th>

                                        <th>Profit</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  
                                    <tr ng-repeat="item in sellTransactions">
                                        <td>{{item.Date}}</td>
                                        <td>{{item.Symbol}}</td>
                                        <td>{{item.Amount}}</td>
                                        <td>{{item.BuyPrice}}</td>
                                        <td>{{item.SellPrice}}</td>
                                        <td>{{item.DiffTotal}}</td>
                                        <td>Sell</td>
                                        <td ng-class="item.PercClass">{{item.Percentage}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                       

                    </uib-tab>
                    <uib-tab index="2"  ng-show="selectedCoin!=undefined"  heading="Update Buy">

                        <div>
                            <h2>Update Buy</h2>
                            <br />
                            Price <input ng-model="selectedCoin.BuyPrice" /> Amount <input ng-model="selectedCoin.Amount" /> <button ng-click="updateCoin('B')" style="width:100px; height:30px;">Update Buy</button>
                           
                          
                        </div>
                    </uib-tab>
                    <uib-tab index="3" ng-show="selectedCoin!=undefined"  heading="Add Sell">
                        <h2>Add Sell</h2>
                          Price <input ng-model="selectedCoin.SellPrice" />  Amount <input ng-model="selectedCoin.SellAmount" />  <button ng-click="sellCoin('S')" style="width:100px; height:30px;">Add Sell</button>
                      
                      
                      
                    </uib-tab>
                    <uib-tab index="4" ng-show="selectedCoin!=undefined" heading="Add Target">
                        <h2>Add Target</h2>
                        Price <input ng-model="selectedCoin.SellPrice" />  Amount <input ng-model="selectedCoin.SellAmount" />  <button ng-click="targetCoin('T')" style="width:100px; height:30px;">Add Target</button>


                    </uib-tab>
               
                   
                </uib-tabset>
               
            </main>
        </div>
    </div>

