                                <div  style="border-bottom: 1px solid #0094ff">Tot: {{item.TargetTotal}} </div>
                                <div>Perc: {{item.TargetPerc}} </div>*@
                                @*<div><button ng-click="sellCoin(item)"  style="width:90px; height:30px;">Sell</button></div>*@
                                <div><button ng-click="detailCoin(item)"  style="width:90px; height:30px;">Detail</button></div>
                                <div><button style="width:90px; height:30px;">Close</button></div>
